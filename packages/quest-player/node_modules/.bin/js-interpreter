#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mac/Downloads/react-quest-monorepo-main 2/node_modules/.pnpm/js-interpreter@6.0.1/node_modules/js-interpreter/lib/node_modules:/Users/mac/Downloads/react-quest-monorepo-main 2/node_modules/.pnpm/js-interpreter@6.0.1/node_modules/js-interpreter/node_modules:/Users/mac/Downloads/react-quest-monorepo-main 2/node_modules/.pnpm/js-interpreter@6.0.1/node_modules:/Users/mac/Downloads/react-quest-monorepo-main 2/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mac/Downloads/react-quest-monorepo-main 2/node_modules/.pnpm/js-interpreter@6.0.1/node_modules/js-interpreter/lib/node_modules:/Users/mac/Downloads/react-quest-monorepo-main 2/node_modules/.pnpm/js-interpreter@6.0.1/node_modules/js-interpreter/node_modules:/Users/mac/Downloads/react-quest-monorepo-main 2/node_modules/.pnpm/js-interpreter@6.0.1/node_modules:/Users/mac/Downloads/react-quest-monorepo-main 2/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../js-interpreter/lib/cli.min.js" "$@"
else
  exec node  "$basedir/../js-interpreter/lib/cli.min.js" "$@"
fi
